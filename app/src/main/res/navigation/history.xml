<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/history"
    app:startDestination="@+id/nav_history">

    <fragment
        android:id="@+id/nav_history"
        android:name="com.fishhawk.driftinglibraryandroid.ui.main.history.HistoryFragment"
        tools:layout="@layout/history_fragment" />

    <action
        android:id="@+id/action_to_gallery"
        app:destination="@id/nav_gallery" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.fishhawk.driftinglibraryandroid.ui.main.gallery.detail.GalleryFragment"
        tools:layout="@layout/gallery_fragment" />
</navigation>